{
	"name": "@lachero/cq",
	"version": "0.0.4",
	"description": "Type-safe CQRS",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/core/index.js",
			"types": "./dist/core/index.d.ts"
		},
		"./client": {
			"import": "./dist/core/client.js",
			"types": "./dist/core/client.d.ts"
		},
		"./internals/*": {
			"import": "./dist/core/internals/*.js",
			"types": "./dist/core/internals/*.d.ts"
		},
		"./vite": {
			"import": "./dist/integrations/vite.js",
			"types": "./dist/integrations/vite.d.ts"
		}
	},
	"scripts": {
		"build": "tsup",
		"build:watch": "tsup --watch",
		"release": "npm run build && npm publish"
	},
	"files": [
		"dist"
	],
	"keywords": [],
	"repository": {
		"type": "git",
		"url": "https://github.com/lacherogwu/cq.git"
	},
	"bugs": {
		"url": "https://github.com/lacherogwu/cq/issues"
	},
	"author": "LacheRo`",
	"license": "MIT",
	"packageManager": "pnpm@10.13.1",
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"peerDependencies": {
		"vite": "^5.0.0 || ^6.0.0 || ^7.0.0"
	},
	"devDependencies": {
		"@types/node": "^24.7.0",
		"tsup": "^8.5.0",
		"tsx": "^4.20.6",
		"typescript": "~5.9.3"
	},
	"dependencies": {
		"@standard-schema/spec": "^1.0.0",
		"fast-glob": "^3.3.3",
		"h3": "2.0.0-beta.5",
		"picocolors": "^1.1.1",
		"superjson": "^2.2.2",
		"vite": "^7.1.9"
	}
}
